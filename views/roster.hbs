
<h1>Roster.</h1>
  
<h2>{{active_team.name}}</h2>

<div class="roster">
  {{#each players}}
    <div>
      <span>{{player_id}}</span> | 
      <span>{{full_name}}</span> | 
      <span>{{number}}</span> | 
      <span>{{phone_number}}</span> | 
      <span>{{position}}</span>
    </div>
  {{/each}}
</div>

{{#section 'head'}}
  <link rel="stylesheet" type="text/css" href="/build/css/nav.bundle.css">
  <link rel="stylesheet" type="text/css" href="/build/css/footer.bundle.css">
  <link rel="stylesheet" type="text/css" href="/build/css/roster.bundle.css">
{{/section}}

{{#section 'scripts'}}
  {{#if isDev}}
    {{!-- set scripts to window to avoid waiting on bundles --}}
  {{else}}
    {{!-- <script src="/build/bundle.js"></script> --}}
  {{/if}}  
{{/section}}
