<h1>{{active_season.name}}</h1>

<div class="games">
  {{#each games}}
    <div class="game">
      <div> id: {{_id}} </div> 
      <div> number: {{number}} </div>
      <div> address: {{address}} </div>
      <div> gym: {{gym}} </div>
      <div> location: {{location_details}} </div>
      <div> opponent: {{opponent}} </div>
      <div> result: {{result}} </div>
    </div>
  {{/each}}
</div>

{{#section 'head'}}
  <link rel="stylesheet" type="text/css" href="/build/css/nav.bundle.css">
  <link rel="stylesheet" type="text/css" href="/build/css/footer.bundle.css">
  <link rel="stylesheet" type="text/css" href="/build/css/schedule.bundle.css">
{{/section}}

{{#section 'scripts'}}
  <script src="/build/js/header.bundle.js"></script>
{{/section}}
